<div class="col-xs-7 col-xs-offset-3">
<%= form_for @lesson do |f| %>
    <div class="box10">
    <h4>担当の教授</h4>
    <%= f.text_field :teacher_name %>
    <h4>授業名</h4>
    <%= f.text_field :title %>
    <h4>授業に対する評価</h4>
    <div class="box14">
    <span><%= f.text_area :body, size: "82x20"%><span>
    </div>
    <br><%= f.submit "投稿する", class:"btn btn-primary btn-block " %>
    </div>
<% end %>
</div>